{
  "name": "blockchain-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test:contracts": "node ./util/truffleHelper.js --test",
    "test:contracts:dev": "export PROFILE='dev'; node ./util/truffleHelper.js --test",
    "test:contracts:test": "export PROFILE='test'; node ./util/truffleHelper.js --test",
    "test:contracts:prod": "export PROFILE='prod'; node ./util/truffleHelper.js --test",
    "test:contracts:uat": "export PROFILE='uat'; node ./util/truffleHelper.js --test",
    "test:api": "mocha --recursive --exit ./spec/**/**/*.js --timeout 90000",
    "test:api:unit": "mocha --recursive --exit ./spec/unit/**/*.js --timeout 90000",
    "test:api:integration": "mocha --recursive --exit ./spec/integration/**/*.js --timeout 90000",
    "test": "npm run test:contracts && npm run test:api",
    "test:dev": "npm run test:contracts:dev && npm run test:api",
    "test:prod": "npm run test:contracts:prod && npm run test:api",
    "test:test": "npm run test:contracts:test && npm run test:api",
    "test:uat": "npm run test:contracts:uat && npm run test:api",
    "test:api:debug": "mocha --recursive --exit --inspect-brk ./spec/**/*.js --timeout 90000",
    "test:junit": "mocha --recursive --exit --timeout 90000 ./spec/**/*.js --reporter mocha-junit-reporter --reporter-options mochaFile=./test-output/test-results.xml",
    "start": "nodemon server.js",
    "start-dev": "export PROFILE='dev'; nodemon server.js",
    "start-test": "export PROFILE='test'; nodemon server.js",
    "start-prod": "export PROFILE='prod'; nodemon server.js",
    "start-uat": "export PROFILE='prod'; nodemon server.js",
    "deploy:contracts": "node ./util/truffleHelper.js --migrate",
    "deploy:contracts:dev": "export PROFILE='dev'; node ./util/truffleHelper.js --migrate",
    "deploy:contracts:prod": "export PROFILE='prod'; node ./util/truffleHelper.js --migrate",
    "deploy:contracts:test": "export PROFILE='test'; node ./util/truffleHelper.js --migrate",
    "deploy:contracts:uat": "export PROFILE='uat'; node ./util/truffleHelper.js --migrate",
    "lint:api": "eslint api/**",
    "lint:contracts": "solium -d contracts/",
    "coverage": "nyc --reporter=lcov mocha --recursive --exit ./spec/**/**/*.js --timeout 90000",
    "coverage:contracts": "./node_modules/.bin/solidity-coverage",
    "generateEthereumAccount": "node ./util/generateEthereumAccount.js",
    "api:stress:test": "node ./util/testMyCareBlockchainApi.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "adal-node": "^0.1.28",
