    "atob": "^2.1.2",
    "axios": "^0.18.0",
    "azure-keyvault": "^3.0.4",
    "azure-storage": "^2.10.2",
    "big-integer": "^1.6.40",
    "body-parser": "^1.18.3",
    "btoa": "^1.2.1",
    "cryptr": "^4.0.2",
    "dotenv": "^5.0.1",
    "dotenv-flow": "^0.1.0",
    "ethereumjs-tx": "^1.3.7",
    "express": "^4.16.4",
    "file-encryptor": "^0.1.1",
    "handlebars": "^4.0.12",
    "jsonwebtoken": "^8.4.0",
    "moment": "^2.22.2",
    "mongoose": "^5.4.3",
    "morgan": "^1.9.1",
    "niceware": "^1.0.5",
    "nodemailer": "^5.1.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "save": "^2.3.3",
    "solium": "^1.1.8",
    "truffle-hdwallet-provider": "0.0.6",
    "ursa": "^0.9.4",
    "uuid": "^3.3.2",
    "web3": "1.0.0-beta.34",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-babel": "^1.2.3",
    "babel-cli": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "commander": "^2.20.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "ethereumjs-wallet": "^0.6.2",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "nodemon": "^1.18.6",
    "nyc": "^13.1.0",
    "proxyquire": "^2.1.0",
    "shelljs": "^0.8.3",
    "sinon": "^7.1.1",
    "solidity-coverage": "^0.5.11",
    "truffle": "^4.1.14"
  }
}
